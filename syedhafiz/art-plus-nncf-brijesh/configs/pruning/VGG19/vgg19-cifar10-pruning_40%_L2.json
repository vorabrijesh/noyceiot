{
    "model": "VGG19",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 32, 32, 3]
    },
    "batch_size": 128,
    "epochs": 10,
    "optimizer": {
        "type": "sgd",
        "schedule_type": "multistep",
        "schedule_params": {
            "base_lr": 0.1,
            "steps": [20, 40, 60],
            "gamma": 0.1
        },
        "optimizer_params": {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "dataset": "cifar10",
    "dataset_type": "tfds",

    "compression": {
        "algorithm": "filter_pruning",
        "pruning_init": 0.1,
        "params": {
            "schedule": "exponential",
            "pruning_target": 0.4,
            "pruning_steps": 15,
            "filter_importance": "L2"
        }
    }
}
