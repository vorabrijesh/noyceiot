{
    "model": "ResNet50",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 32, 32, 3]
    },
    "batch_size": 128,
    "epochs": 2,
    "optimizer": {
        "type": "Adam",
        "schedule_type": "piecewise_constant",
        "schedule_params": {
            "boundaries": [2],
            "values": [1e-5, 1e-6]
        }
    },
    "dataset": "cifar10",
    "dataset_type": "tfds",

   "compression": {
        "algorithm": "quantization",
        "initializer": {
            "batchnorm_adaptation": {
                "num_bn_adaptation_samples": 2048
            }
        },
        "weights": {
            "per_channel": false
        },
        "activations": {
            "per_channel": false
        }
    }
}
